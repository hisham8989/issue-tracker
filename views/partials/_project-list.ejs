<ul id="project-list">
  <% for( let project of projects ) { %>
  <a href="/project/<%=project._id%>">
    <li>
      <table class="rwd-table">
        <tr>
          <th>Name of project</th>
          <th>Description</th>
          <th>Author</th>
        </tr>
        <tr>
          <td data-th="Name of project"><%= project.name %></td>
          <td data-th="Description"><%= project.description %></td>
          <td data-th="Author"><%= project.author %></td>
        </tr>
      </table>
    </li></a
  >
  <br />
  <% } %>
</ul>
